# video_streaming_client-config.cmake.in
# Config file for the video_streaming_client package

@PACKAGE_INIT@

# Find dependencies
find_package(holoscan 3.5 REQUIRED CONFIG)

# Add the current directory to CMAKE_MODULE_PATH for FindStreamingClient.cmake
list(APPEND CMAKE_MODULE_PATH "${CMAKE_CURRENT_LIST_DIR}")

# Find StreamingClient
find_package(StreamingClient REQUIRED)

# Include the targets file
include("${CMAKE_CURRENT_LIST_DIR}/video_streaming_client-targets.cmake")

# Check that the targets exist
check_required_components(video_streaming_client) 