# video_streaming_server-config.cmake.in
# Config file for the video_streaming_server package

@PACKAGE_INIT@

# Find dependencies
find_package(holoscan 3.2 REQUIRED CONFIG)

# Add the StreamingServer module path
list(APPEND CMAKE_MODULE_PATH "${CMAKE_CURRENT_LIST_DIR}")

# Find StreamingServer
find_package(StreamingServer REQUIRED)

# Include the targets file
include("${CMAKE_CURRENT_LIST_DIR}/video_streaming_server-targets.cmake")

# Check that the targets exist
check_required_components(video_streaming_server) 
